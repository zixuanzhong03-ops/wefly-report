        var chartOpts = function(title) {
            return {
                responsive: true,
                maintainAspectRatio: false,
                plugins: {
                    legend: { position: 'bottom', labels: { color: '#1F2937' } },
                    title: { display: true, text: title, color: '#1F2937' }
                }
            };
        };

        var barOpts = function(title) {
            var opts = chartOpts(title);
            opts.indexAxis = 'y';
            opts.scales = {
                x: { beginAtZero: true, max: 100, ticks: { callback: function(v) { return v + '%'; }, color: '#1F2937' }, grid: { color: '#E5E7EB' } },
                y: { ticks: { color: '#1F2937' }, grid: { color: '#E5E7EB' } }
            };
            return opts;
        };

